<% @images.each do |image| %>
  $('#news-feed-images')
    .append('<%= j render "images/image", image: image %>');
<% end %>
<% if @last %>
  $('#load-more-news-feed').hide();
  $('.news-feed-middle-content').removeClass('with-padding');
<% else %>
  $('#load-more-news-feed')
    .html('<%= link_to t("image_action.load_more"), user_book_marks_path(@user,
    image_offset: @images.last.id), remote: true %>');
<% end %>
